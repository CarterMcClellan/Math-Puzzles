   "source": [
    "# a = m^2 - n^2\n",
    "# b = 2mn\n",
    "# c = n^2 + m^2\n",
    "def generate_triples(N):\n",
    "    triples = set()\n",
    "    for m in range(2, N):\n",
    "        for n in range(1, m):\n",
    "            triples.add((m**2 - n**2, 2*m*n, n**2 + m**2))\n",
    "            \n",
    "    return triples"
   ]
